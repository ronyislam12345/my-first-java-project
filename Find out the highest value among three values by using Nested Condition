import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num1 = 15;
        int num2 = 90;
        int num3 = 30;

        int highest;
        // Find the highest value using nested if-else statements
        if (num1 >= num2) {
            if (num1 >= num3) {
                highest = num1;
            } else{
                highest = num3;
            }
        } else{
            if (num2 >= num3) {
                highest = num2;
            } else{
                highest = num3;
            }
        }
        System.out.println(highest);
    }
}
